{
  "name": "@payloadcms/payload-plugin-oauth",
  "version": "0.1.0",
  "author": "Sourab Pramanik<shubpramanik241@gmail.com>",
  "license": "MIT",
  "homepage:": "https://github.com/sourabpramanik/plugin-payload-oauth",
  "repository": "git@github.com:sourabpramanik/plugin-payload-oauth.git",
  "description": "Oauth plugin for Payload CMS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "typescript",
    "react",
    "oauth",
    "oauth2.0",
    "oidc",
    "google",
    "github",
    "gitlab",
    "atlassian"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "default": "./dist/providers/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cd dev && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build":  "rm -rf dist && rm -rf tsconfig.tsbuildinfo && pnpm build:types && pnpm build:esbuild",
    "build:esbuild": "node bundle.js",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "test": "cd test && jest --config=./jest.config.js",
    "lint": "eslint src",
    "payload": "cd dev && cross-env NODE_OPTIONS=--no-deprecation payload",
    "dev:lint": "cd dev && cross-env NODE_OPTIONS=--no-deprecation next lint",
    "generate:types": "payload generate:types",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "clean": "rm -rf dist && rm -rf dev/pnpm-lock.yaml",
    "prepublish": "yarn lint:fix && yarn clean && yarn build"
  },
  "peerDependencies": {
    "payload": "^3.0.0-beta.60"
  },
  "devDependencies": {
    "@payloadcms/db-mongodb": "^3.0.0-beta.67",
    "@payloadcms/eslint-config": "^1",
    "@payloadcms/next": "^3.0.0-beta.67",
    "@payloadcms/richtext-lexical": "^3.0.0-beta.67",
    "@payloadcms/ui": "^3.0.0-beta.67",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/react": "^18.2.78",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "esbuild": "0.23.0",
    "esbuild-css-modules-plugin": "^3.1.2",
    "esbuild-node-externals": "^1.14.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "next": "15.0.0-rc.0",
    "payload": "^3.0.0-beta.67",
    "prettier": "^2.7.1",
    "react": "^19.0.0-rc-85acf2d195-20240711",
    "sharp": "^0.33.4",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "oauth4webapi": "^2.11.1",
    "react-dom": "^19.0.0-rc-85acf2d195-20240711"
  }
}
