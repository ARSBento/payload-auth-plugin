{
  "name": "payload-auth-plugin",
  "version": "0.5.0",
  "type": "module",
  "sideEffects": false,
  "author": "Sourab Pramanik<shubpramanik241@gmail.com>",
  "license": "MIT",
  "homepage:": "https://github.com/sourabpramanik/payload-auth-plugin",
  "repository": "git@github.com:sourabpramanik/payload-auth-plugin.git",
  "description": "Authentication plugin for Payload CMS",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "keywords": [
    "payload",
    "cms",
    "authentication",
    "plugin",
    "typescript",
    "react",
    "nextjs",
    "oauth",
    "oauth2.0",
    "oidc",
    "google",
    "github",
    "gitlab",
    "atlassian",
    "apple",
    "facebook",
    "discord",
    "auth0",
    "cognito",
    "keycloak",
    "microsoft",
    "slack",
    "passkey",
    "webauthn"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts",
      "default": "./dist/providers/index.js"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run script/build.ts"
  },
  "peerDependencies": {
    "payload": "latest"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.10",
    "@tsconfig/node22": "22.0.0",
    "@types/bun": "latest",
    "@types/jsonwebtoken": "^9.0.7",
    "cross-env": "^7.0.3",
    "globals": "^15.14.0",
    "prettier": "3.4.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.0",
    "jsonwebtoken": "^9.0.2",
    "oauth4webapi": "^3.1.4"
  }
}